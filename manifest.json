{
  "manifest_version": 3,
  "name": "Text Buffer",
  "description": "Extension for quick access to text",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "public/popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "background": {}
}